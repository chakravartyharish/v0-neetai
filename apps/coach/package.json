    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "analyze": "ANALYZE=true npm run build",
    "db:generate-types": "supabase gen types typescript --project-id=$NEXT_PUBLIC_SUPABASE_PROJECT_ID --schema=public > src/types/supabase.ts",
